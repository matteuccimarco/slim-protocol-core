{
  "name": "slim-protocol-core",
  "version": "0.1.0",
  "description": "SLIM (Structured Lightweight Interchange Markup) - Token-efficient data serialization for AI/LLM",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "benchmark": "tsx benchmarks/run.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "slim",
    "serialization",
    "json",
    "llm",
    "ai",
    "token",
    "compression",
    "data-format"
  ],
  "author": "SLIM Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matteuccimarco/slim-protocol-core.git"
  },
  "homepage": "https://github.com/matteuccimarco/slim-protocol-core#readme",
  "bugs": {
    "url": "https://github.com/matteuccimarco/slim-protocol-core/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
